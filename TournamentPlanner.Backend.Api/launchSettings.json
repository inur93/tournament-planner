{
  "profiles": {
    "Docker Compose": {
      "commandName": "DockerCompose",
      "commandVersion": "1.0",
      "composeLaunchAction": "LaunchBrowser",
      "composeLaunchServiceName": "tournamentplanner.backend.api",
      "composeLaunchUrl": "{Scheme}://localhost:{ServicePort}/swagger",
      "useSSL": true,
      "httpPort": 5000,
      "sslPort": 5001,
      "serviceActions": {
        "db": "StartWithoutDebugging",
        "pgadmin": "StartWithoutDebugging",
        "tournamentplanner.backend.api": "StartDebugging"
      }
    }
  }
}